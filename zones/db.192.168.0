;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	inferno.navi. root.inferno.navi. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.inferno.navi.
10	IN	PTR	ns1.inferno.navi.
10	IN	PTR	lab.inferno.navi.
56	IN	PTR	ns1.rayquen.navi.
56	IN	PTR	lab.rayquen.navi.
56	IN	PTR	mail.rayquen.navi.
