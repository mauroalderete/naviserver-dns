;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.rayquen.wired. root.rayquenx.wired. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

; name servers - NS records
@	IN	NS	ns1.rayquenx.wired.

; name servers - A records
@	IN	A	192.168.0.57
ns1	IN	A	192.168.0.57
mx	IN	A	192.168.0.57

; 192.168.0.0/24 - A records
test.rayquenx.wired.	IN	A	192.168.0.57

