;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.rayquen.navi. root.rayquen.navi. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

; name servers - NS records
@	IN	NS	ns1.rayquen.navi.

; name servers - A records
@	IN	A	192.168.0.56
ns1	IN	A	192.168.0.56
mx	IN	A	192.168.0.56

; 192.168.0.0/24 - A records
lab.rayquen.navi.	IN	A	192.168.0.56
mail.rayquen.navi.	IN	A	192.168.0.56

