;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.inferno.navi. root.inferno.navi. (
			      7		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

; name servers - NS records
@	IN	NS	ns1.inferno.lan.

; name servers - A records
@	IN	A	192.168.0.10
ns1	IN	A	192.168.0.10
mx	IN	A	192.168.0.10

; 192.168.0.0/24 - A records
lab.inferno.navi.	IN	A	192.168.0.10
mail.inferno.navi.	IN	A	192.168.0.10

